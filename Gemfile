source "https://rubygems.org"

# As the jekyll package is a dependency of github-pages the following line
# fixes jekyll to the version that the current Github Pages release supports
# To upgrade, run `bundle update github-pages`.
gem "github-pages", group: :jekyll_plugins

# If you have any plugins, put them here!
group :jekyll_plugins do
  gem 'jekyll-paginate'
  gem 'jekyll-sitemap'
  gem 'jekyll-gist'
  gem 'jekyll-feed'
  gem 'jekyll-include-cache'
end

